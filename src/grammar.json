{
  "name": "haskell_persistent",
  "rules": {
    "quasi_quotation": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_entity_definition"
      }
    },
    "_entity_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_entity_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_entity_line_definition"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_entity_derives"
          }
        }
      ]
    },
    "_newline": {
      "type": "PATTERN",
      "value": "\\n|\\r|\\f"
    },
    "is_sum_marker": {
      "type": "STRING",
      "value": "+"
    },
    "_varid": {
      "type": "PATTERN",
      "value": "[_\\p{Ll}](\\w|')*#?"
    },
    "_conid": {
      "type": "PATTERN",
      "value": "[\\p{Lu}\\p{Lt}](\\w|')*"
    },
    "_doc_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "-- | "
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "comment": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_doc_comment"
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "--"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "#"
              },
              {
                "type": "PATTERN",
                "value": ".*"
              }
            ]
          }
        }
      ]
    },
    "_entity_name": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "_conid"
      },
      "named": true,
      "value": "identifier"
    },
    "field_name": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "_varid"
      },
      "named": true,
      "value": "identifier"
    },
    "_haskell_constraint_name": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "_conid"
      },
      "named": true,
      "value": "identifier"
    },
    "_entity_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "is_sum_marker"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_entity_name"
        },
        {
          "type": "SYMBOL",
          "name": "_entity_attributes"
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_entity_line_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_entity_key"
            },
            {
              "type": "SYMBOL",
              "name": "_field_definition"
            },
            {
              "type": "SYMBOL",
              "name": "_unique_constraint"
            },
            {
              "type": "SYMBOL",
              "name": "_foreign_constraint"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_newline"
        }
      ]
    },
    "_entity_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_surrogate_key"
        },
        {
          "type": "SYMBOL",
          "name": "_natural_key"
        }
      ]
    },
    "_surrogate_key": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Id"
        },
        {
          "type": "SYMBOL",
          "name": "field_type"
        },
        {
          "type": "SYMBOL",
          "name": "_key_attributes"
        }
      ]
    },
    "_natural_key": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Primary"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "field_name"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_key_attributes"
        }
      ]
    },
    "_key_attributes": {
      "type": "SYMBOL",
      "name": "_attributes"
    },
    "_entity_attributes": {
      "type": "SYMBOL",
      "name": "_attributes"
    },
    "_field_attributes": {
      "type": "SYMBOL",
      "name": "_attributes"
    },
    "_attributes": {
      "type": "PATTERN",
      "value": "sql=\\w+"
    },
    "_field_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_field_strictness_prefix"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_varid"
          },
          "named": true,
          "value": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "field_type"
        },
        {
          "type": "SYMBOL",
          "name": "_field_attributes"
        }
      ]
    },
    "_field_strictness_prefix": {
      "type": "PATTERN",
      "value": "[~!]"
    },
    "field_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_conid"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "STRING",
              "value": "["
            },
            {
              "type": "SYMBOL",
              "name": "_conid"
            },
            {
              "type": "STRING",
              "value": "]"
            }
          ]
        }
      ]
    },
    "_cascade_action": {
      "type": "PATTERN",
      "value": "OnDelete\\w+|OnUpdate\\w+"
    },
    "_cascade_actions": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_cascade_action"
      }
    },
    "_unique_constraint": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_haskell_constraint_name"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "field_name"
          }
        },
        {
          "type": "SYMBOL",
          "name": "_unique_constraint_attributes"
        }
      ]
    },
    "_unique_constraint_attributes": {
      "type": "SYMBOL",
      "name": "_attributes"
    },
    "_constraint_name": {
      "type": "SYMBOL",
      "name": "_varid"
    },
    "_foreign_constraint": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "Foreign"
        },
        {
          "type": "SYMBOL",
          "name": "_entity_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_cascade_actions"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "_constraint_name"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "field_name"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "References"
                },
                {
                  "type": "REPEAT1",
                  "content": {
                    "type": "SYMBOL",
                    "name": "field_name"
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_class_name": {
      "type": "ALIAS",
      "content": {
        "type": "SYMBOL",
        "name": "_conid"
      },
      "named": true,
      "value": "identifier"
    },
    "_entity_derives": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "deriving"
        },
        {
          "type": "REPEAT1",
          "content": {
            "type": "SYMBOL",
            "name": "_class_name"
          }
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\p{Zs}"
    },
    {
      "type": "PATTERN",
      "value": "\\n"
    },
    {
      "type": "PATTERN",
      "value": "\\r"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

